        "sample = \"A lot of things occur each day, it's very dynamical every day.\"\n",
        "pos = sample.find('day')\n",
        "print(pos)\n",
        "\n",
        "spos = 0       # find()가 대상을 찾기 시작할 인덱스\n",
        "idx = -1       # 대상이 위치하는 인덱스\n",
        "count = 1\n",
        "while idx != -1:\n",
        "    idx = sample.find('day', spos)\n",
        "    print(f\"'day'가 발견된 [{count}]번째 인덱스 {idx}\")\n",
        "    spos = idx + 1   # find()가 대상을 찾기 시작할 인덱스 수정\n",
        "    count += 1\n"
      ]
    }
